{
  "apiVersion":"apps/v1",
  "kind":"ReplicaSet",
  "metadata":{
    "name":"%s",
    "namespace":"kube-system",
    "tenant":"system"
  },
  "spec":{
    "replicas":%d,
    "selector":{
      "matchLabels":{
        "ln":"%s"
      }
    },
    "template":{
      "metadata":{
        "annotations":{
          "VirtletCPUModel":"host-model"
        },
        "labels":{
          "ln":"%s"
        }
      },
      "spec":{
        "virtualMachine":{
          "image":"%s",
          "imagePullPolicy":"IfNotPresent",
          "keyPairName":"foobar",
          "name":"%s",
          "resources":{
            "limits":{
              "cpu":"%d",
              "memory":"%dMi"
            },
            "requests":{
              "cpu":"%d",
              "memory":"%dMi"
            }
          }
        }
      }
    }
  }
}